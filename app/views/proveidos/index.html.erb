<p id="notice"><%= notice %></p>

<h1>Proveidos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proveidos.each do |proveido| %>
      <tr>
        <td><%= proveido.nombre %></td>
        <td><%= link_to 'Show', proveido %></td>
        <td><%= link_to 'Edit', edit_proveido_path(proveido) %></td>
        <td><%= link_to 'Destroy', proveido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proveido', new_proveido_path %>
